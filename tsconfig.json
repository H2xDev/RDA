{
  "compilerOptions": {
   "target": "es5",
   "lib": ["ES2017", "DOM"],
   "module": "commonjs",
   "moduleResolution": "node",
   "outDir": "public",
   "forceConsistentCasingInFileNames": true,
   "strict": true,
   "skipLibCheck": true,
   "noImplicitAny": false,
   "esModuleInterop": true
  }
}
